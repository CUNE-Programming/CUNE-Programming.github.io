---
/*
 * RosterImage.astro
 * Ian Kollipara
 * 2023-08-13
 *
 * Image Component for Rosters
 */

import { Image } from "astro:assets";
import type { ImageMetadata } from "astro";

type Props = {
  img: ImageMetadata;
  alt: string;
};

const { img: src, alt } = Astro.props;
---

<Image
  {src}
  width={src.width}
  height={src.height}
  class="mx-auto p-10 object-contain"
  {alt}
/>
